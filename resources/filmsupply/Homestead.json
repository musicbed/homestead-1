{
    "name": "filmsupply",
    "ip": "10.1.1.34",
    "memory": 4096,
    "cpus": 4,
    "provider": "virtualbox",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": [
        "~/.ssh/id_rsa"
    ],
    "features": [
        {
            "elasticsearch": {
                "version": "7"
            },
            "ohmyzsh": true
        }
    ],
    "folders": [
        {
            "map": "~/Code/musicbed/filmsupply",
            "to": "/home/vagrant/filmsupply",
            "type": "nfs"            
        }             
    ],
    "sites": [
        {
            "map": "api.filmsupply.test",
            "to": "/home/vagrant/filmsupply/public/api",
            "php": "7.4"
        }
    ],
    "databases": [
        "filmsupply"
    ],
    "ports": [
        {
            "send": "8003",
            "to": "80"
        },
        {
            "send": "33063",
            "to": "3306"
        }
    ]
}
